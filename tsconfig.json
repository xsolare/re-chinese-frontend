{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "#app": ["./node_modules/nuxt3/dist/app/index"],
      "#/*": ["./*"],
      "#components/*": ["./components/*"],
      "#composables/*": ["./composables/*"],
      "#types/*": ["./types/*"],
      "#plugins/*": ["./plugins/*"]
    }
  }
}
